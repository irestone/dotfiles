# ALIASES

# General

alias c='clear'

# Git

alias gs='git status'
alias gl='git log --graph --oneline --decorate'

alias gd='git diff'
alias gds='git diff --staged'
alias gdh='git diff HEAD'

alias ga='git add'
alias gaa='git add -A'
alias gi='git add -i'

alias gc='git commit -m'

alias gb='git branch'
alias gba='git branch -a'
alias gbd='git branch -d'
alias gbD='git branch -D'

alias go='git checkout'

# NPM

alias nr='npm run'
alias nrd='npm run dev'
